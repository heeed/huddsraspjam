            <ITEXT FONT="Arial Bold" CH="Scratch GPIO"/>
            <para/>
            <para/>
            <ITEXT CH="To make the LED flash we are going to use a variant of Scratch which has been modified to be able to use the GPIO pins called scratchGPIO7."/>
            <para/>
            <para/>
            <para/>
            <ITEXT CH="Scratch GPIO automatically configures the following pins:"/>
            <para/>
            <para/>
            <tab/>
            <ITEXT CH="For output:"/>
            <tab/>
            <ITEXT CH="11,12,13,14,15,16,18,21"/>
            <para/>
            <tab/>
            <ITEXT CH="For input:"/>
            <tab/>
            <ITEXT CH="7,10,19,22,24,26"/>
            <para/>
            <tab/>
            <ITEXT FONTSIZE="9" CH="(Scratch GPIO uses the physical pin numbers...the one in the circles)"/>
            <para/>
            <para/>
            <ITEXT FONT="Arial Bold" CH="Sending an output:"/>
            <para/>
            <para/>
            <ITEXT CH="To send values to the output pins we use the broadcast block to broadcast the following commands:"/>
            <para/>
            <para/>
            <tab/>
            <ITEXT CH="Command"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Example"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Result"/>
            <para/>
            <tab/>
            <ITEXT CH="pin"/>
            <ITEXT FONT="Arial Italic" CH="X"/>
            <ITEXT CH="on"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="pin11on"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Turns pin"/>
            <ITEXT FONT="Arial Italic" CH=" X"/>
            <ITEXT CH=" on"/>
            <para/>
            <tab/>
            <ITEXT CH="pin"/>
            <ITEXT FONT="Arial Italic" CH="X"/>
            <ITEXT CH="off"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="pin11off"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Turns pin "/>
            <ITEXT FONT="Arial Italic" CH="X"/>
            <ITEXT CH=" off"/>
            <para/>
            <tab/>
            <ITEXT CH="allon"/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Turn all pins on"/>
            <para/>
            <tab/>
            <ITEXT CH="alloff"/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Turn all pins off"/>
            <para/>
            <tab/>
            <ITEXT CH="pinpatternXXXXXXX"/>
            <tab/>
            <ITEXT CH="See below"/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="1 for on / 0 for off"/>
            <para/>
            <para/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Need pin 21,16,12,11 on and all others off:"/>
            <para/>
            <tab/>
            <tab/>
            <tab/>
            <tab/>
            <para/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="Pins:"/>
            <tab/>
            <tab/>
            <ITEXT CH="21|18|16|15|14|13|12|11"/>
            <para/>
            <ITEXT CH=" "/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="On/Off:"/>
            <tab/>
            <ITEXT CH="1  | 0 | 1 | 0 | 0 | 0 | 1 | 1"/>
            <para/>
            <tab/>
            <tab/>
            <tab/>
            <para/>
            <tab/>
            <tab/>
            <tab/>
            <ITEXT CH="pinpattern10100011"/>
            <para/>
            <para/>
